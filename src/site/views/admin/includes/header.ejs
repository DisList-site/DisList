    <!-- Preloader -->
    <div class="preloader flex-column justify-content-center align-items-center" style="height: 0px;">
        <img class="animation__shake" src="https://welcome-bot.github.io/assets/img/logo.png" height="60" widgth="60" style="display: none;">
    </div>
    <!-- Navbar -->
    <nav class="main-header navbar navbar-expand navbar-white navbar-light">
        <!-- Left Navbar links -->
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="nav-link" data-widget="pushmenu" href="#"><i class="fas fa-bars"></i></a>
            </li>
        </ul>
        <!-- Right navbar links -->
        <ul class="navbar-nav ml-auto">
            <li class="nav-item d-none d-sm-inline-block"> <!-- Will not be shown on phones -->
                <a class="nav-link" href="https://discord.gg/x54vQG2Hpm" role="button">
                    Discord Support Server
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" data-widget="fullscreen" href="#" role="button">
                    <i class="fas fa-expand-arrows-alt"></i>
                </a>
            </li>
        </ul>
    </nav>
    <!-- Main Sidebar Container -->
    <aside class="main-sidebar sidebar-dark-primary">
        <!-- Brand Logo -->
        <a href="/" class="brand-link">
            <img src="https://welcome-bot.github.io/assets/img/logo.png" alt="Welcome-Bot Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
            <span class="brand-text font-weight-dark"><b>Welcome-Bot</b></span>
        </a>
        <!-- Sidebar -->
        <div class="sidebar">
            <% if (user) { %>
            <!-- Sidebar user panel -->
            <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                <div class="image">
                    <img src="<%= user.displayAvatarURL({size: 32}) %>" class="img-circle elevation-2" alt="User Image">
                </div>
                <div class="info">
                    <a href="#" class="d-block"><%= user.tag %></a>
                </div>
            </div>
            <% } else { %>
            <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                <div class="image">
                    <img src="https://emoji.gg/assets/emoji/discord.png" class="img-circle elevation-2" alt="User Image">
                </div>
                <div class="info">
                    <a href="/login" class="d-block">Login</a>
                </div>
            </div>
            <% } %>
            <!-- Sidebar Menu -->
            <nav class="mt-2">
                <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu">
                    <li class="nav-item menu-open">
                        <a href="/dashboard" class="nav-link">
                            <i class="fas fa-tachometer-alt"></i>
                            <p>
                                <%= translate("dashboard:dashboard") %>
                            </p>
                        </a>
                        <a href="/profile" class="nav-link">
                            <i class="fas fa-user"></i>
                            <p>
                                <%= translate("misc:profile") %>
                                <span class="right badge badge-danger">New</span>
                            </p>
                        </a>
                        <a href="/settings" class="nav-link">
                            <i class="fas fa-cog"></i>
                            <p>
                                Settings
                                <span class="right badge badge-danger">New</span>
                            </p>
                        </a>
                    </li>
                </ul>
            </nav>
        </div>
    </aside>
